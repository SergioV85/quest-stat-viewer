{
  "name": "quest-stat-viewer",
  "version": "2.0.0",
  "license": "MIT",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:prod",
    "build:prod": "ng build --prod",
    "build:prod:universal": "ng run quest-stat-viewer:server:production",
    "build:all": "npm run build:prod && npm run build:prod:universal",
    "build:ssr": "npm run build:all && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "test:coverage": "ng test --source-map --code-coverage",
    "test:ci": "ng test --code-coverage --karma-config ./karma.ci.conf.js && npm run prepare-code-coverage-report",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:heroku": "node dist/server.js",
    "prettier": "prettier --config ./.prettierrc --list-different \"./src/app/**/*.{ts,js,scss,css,json}\"",
    "precommit": "npm run prettier && npm run lint",
    "prepare-code-coverage-report": "replace '...*src' 'SF:./src' coverage/lcov.info",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "build:serverless:deploy:all": "npm run build:ssr && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:prod": "ng run quest-stat-viewer:server && webpack --config webpack.server.config.js --progress --colors",
    "build:server:serverless": "ng run quest-stat-viewer:server:serverless && webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.10",
    "@angular/cdk": "11.2.9",
    "@angular/common": "11.2.10",
    "@angular/compiler": "11.2.10",
    "@angular/core": "11.2.10",
    "@angular/forms": "11.2.10",
    "@angular/material": "11.2.9",
    "@angular/platform-browser": "11.2.10",
    "@angular/platform-browser-dynamic": "11.2.10",
    "@angular/platform-server": "11.2.10",
    "@angular/router": "11.2.10",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@ng-toolkit/serverless": "^8.1.0",
    "@ngrx/effects": "11.1.1",
    "@ngrx/router-store": "11.1.0",
    "@ngrx/store": "11.1.0",
    "@ngrx/store-devtools": "11.1.0",
    "@nguniversal/common": "^11.2.1",
    "@nguniversal/express-engine": "^11.2.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "aws-serverless-express": "^3.4.0",
    "bootstrap": "^4.3.0",
    "core-js": "^3.0.1",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "date-fns": "^2.21.0",
    "express": "^4.17.1",
    "luxon": "^1.26.0",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.9",
    "@angular-eslint/builder": "2.1.0",
    "@angular-eslint/eslint-plugin-template": "2.1.0",
    "@angular-eslint/eslint-plugin": "2.1.0",
    "@angular-eslint/schematics": "2.1.0",
    "@angular-eslint/template-parser": "2.1.0",
    "@angular/cli": "11.2.9",
    "@angular/compiler-cli": "11.2.10",
    "@angular/language-service": "11.2.10",
    "@ngrx/schematics": "11.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.3",
    "@types/luxon": "^1.15.2",
    "@types/node": "~12.0.0",
    "@types/ramda": "^0.27.40",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-react": "latest",
    "eslint-plugin-rxjs": "latest",
    "eslint": "^7.6.0",
    "husky": "~6.0.0",
    "jasmine-core": "~3.7.0",
    "jasmine-marbles": "^0.8.0",
    "jasmine-spec-reporter": "~7.0.0",
