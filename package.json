{
  "name": "quest-stat-viewer",
  "version": "2.0.0",
  "license": "MIT",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:prod",
    "build:prod": "ng build --prod",
    "build:prod:universal": "ng run quest-stat-viewer:server:production",
    "build:all": "npm run build:prod && npm run build:prod:universal",
    "build:ssr": "npm run build:all && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "test:coverage": "ng test --source-map --code-coverage",
    "test:ci": "ng test --code-coverage --karma-config ./karma.ci.conf.js && npm run prepare-code-coverage-report",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:heroku": "node dist/server.js",
    "prettier": "prettier --config ./.prettierrc --list-different \"./src/app/**/*.{ts,js,scss,css,json}\"",
    "precommit": "npm run prettier && npm run lint",
    "prepare-code-coverage-report": "replace '...*src' 'SF:./src' coverage/lcov.info",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "build:serverless:deploy:all": "npm run build:ssr && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:prod": "ng run quest-stat-viewer:server && webpack --config webpack.server.config.js --progress --colors",
    "build:server:serverless": "ng run quest-stat-viewer:server:serverless && webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.0-next.9",
    "@angular/cdk": "11.2.9",
    "@angular/common": "12.0.0-next.9",
    "@angular/compiler": "12.0.0-next.9",
    "@angular/core": "11.2.10",
    "@angular/forms": "12.0.0-next.9",
    "@angular/material": "11.2.9",
    "@angular/platform-browser": "12.0.0-next.9",
    "@angular/platform-browser-dynamic": "12.0.0-next.9",
    "@angular/platform-server": "12.0.0-next.9",
    "@angular/router": "12.0.0-next.9",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@ng-toolkit/serverless": "^7.1.2",
    "@ngrx/effects": "11.1.0",
    "@ngrx/router-store": "11.1.0",
    "@ngrx/store": "11.1.0",
    "@ngrx/store-devtools": "11.1.0",
    "@nguniversal/common": "^10.0.1",
    "@nguniversal/express-engine": "^10.0.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "aws-serverless-express": "^3.2.0",
    "bootstrap": "^4.3.0",
    "core-js": "^3.0.1",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "date-fns": "^2.3.0",
    "express": "^4.17.1",
    "luxon": "^1.19.3",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.9",
    "@angular/cli": "11.2.9",
    "@angular/compiler-cli": "12.0.0-next.9",
    "@angular/language-service": "12.0.0-next.9",
    "@ngrx/schematics": "11.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.3",
    "@types/luxon": "^1.15.2",
    "@types/node": "~10.0.0",
    "@types/ramda": "^0.26.26",
    "angular-tslint-rules": "^1.20.0",
    "codelyzer": "^6.0.0",
    "husky": "~3.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "^0.0.32",
    "node-sass": "^4.12.0",
    "opencollective": "^1.0.3",
    "prettier": "^1.18.2",
    "protractor": "~7.0.0",
    "puppeteer": "^1.15.0",
    "replace": "^1.1.1",
    "rxjs-tslint-rules": "^4.25.0",
    "serverless": "1.40.0",
    "serverless-apigw-binary": "^0.4.4",
    "ts-loader": "^6.0.0",
    "ts-node": "~8.1.0",
    "tslint": "~6.1.3",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.1.5",
    "webpack-cli": "2.1.2"
  }
}
