    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-jasmine": "~4.0.0",
    "karma-spec-reporter": "^0.0.32",
    "karma": "~6.3.2",
    "opencollective": "^1.0.3",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "puppeteer": "^1.15.0",
    "replace": "^1.1.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless": "1.40.0",
    "ts-loader": "^6.0.0",
    "ts-node": "~8.1.0",
    "typescript": "4.1.5",
    "webpack-cli": "2.1.2"
  }
}
